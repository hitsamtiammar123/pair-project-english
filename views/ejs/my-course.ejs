<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>English Senang-Senang</title>
    <%-include("partial/source-head")%>
</head>
<body>
    <%-include("partial/navbar",{session:session})%>
    <div class="container course-hehe">
        <div class="row">
            <% courses.forEach(course=>{%>
            <div class="col-sm-6 course-content">
                <div class="col-sm-12 text-center">
                    <i class="ion-ios-people-outline icon-courses color-green"></i>
                </div>
                <div class="col-sm-12">
                   <h2><%=course.name%></h2>
                   <p>
                       <%=course.summary_text%>
                   </p>
                </div>
            </div>
            <%})%>
        </div>
    </div>
    <%-include("partial/footer")%>
</body>
</html>